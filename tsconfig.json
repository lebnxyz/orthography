{
    "extends": "@tsconfig/svelte/tsconfig.json",
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules/*",
        "public/*"
    ],
    "compilerOptions": {
        "strict": true,
        "noImplicitAny": true,
        "importsNotUsedAsValues": "preserve",
        "exactOptionalPropertyTypes": true,
    }
}
